  <data name="App_DataDictionary_procSetDomainAttributeAlias_Insert_TestAction.SqlScript" xml:space="preserve">
    <value>Begin Try;
	Begin Transaction;
	Set NoCount On;

	Declare @AttributeId UNIQUEIDENTIFIER = newId(),
			@ModelId     UNIQUEIDENTIFIER = newId()
	Declare @Model [App_DataDictionary].[typeModel]
	Declare @Attribute [App_DataDictionary].[typeDomainAttribute]
	
	Insert Into @Model Values (@ModelId, 'Unit Test', Null)
	Exec [App_DataDictionary].[procSetModel] @ModelId, @Model

	Insert Into @Attribute Values (@AttributeId, Null, 'UnitTest', Null)
	Exec [App_DataDictionary].[procSetDomainAttribute] @ModelId, @Attribute

	Insert Into [App_DataDictionary].[AliasSource] ([AliasSourceId], [AliasSourceTitle], [DatabaseName], [IsCaseSensitive])
	Select	NewId(), 'UnitTest', Db_Name(), 0

	Declare @Alias [App_DataDictionary].[typeDomainAttributeAlias]
	Insert Into @Alias
	Select	@AttributeId As [AttributeId],
			Db_Name() As [SourceName],
			FormatMessage('%s.%s.[%s].[%s]', Db_Name(), Object_Schema_Name(object_Id), Object_Name(object_id), name) As [AliasName],
			'Database.Schema.Table.Column' As [ScopeName]
	From	Sys.Columns
	Where	Object_Schema_Name(object_id) = 'App_DataDictionary'

	Exec [App_DataDictionary].[procSetDomainAttributeAlias] @ModelId, @AttributeId, @Alias

	Select	*
	From	[App_DataDictionary].[AliasDomain] A
			Cross Apply [App_DataDictionary].[funcGetAliasName](A.[AliasId]) F
	Where	[AttributeId] = @AttributeId

	-- By default, throw and error and exit without committing
;	Throw 50000, 'Abort process, comment out this line when ready to actual Commit the transaction',255;
	
	Commit Transaction;
	Print 'Commit Issued';
End Try
Begin Catch
	Print FormatMessage ('*** Error Report: %s ***', Object_Name(@@ProcID));
	Print FormatMessage (' Message- %s', ERROR_MESSAGE());
	Print FormatMessage (' Number- %i', ERROR_NUMBER());
	Print FormatMessage (' Severity- %i', ERROR_SEVERITY());
	Print FormatMessage (' State- %i', ERROR_STATE());
	Print FormatMessage (' Procedure- %s', ERROR_PROCEDURE());
	Print FormatMessage (' Line- %i', ERROR_LINE());
	Print FormatMessage (' @@TranCount - %i', @@TranCount);
	Print FormatMessage (' @@NestLevel - %i', @@NestLevel);
	Print FormatMessage (' Original_Login - %s', Original_Login());
	Print FormatMessage (' Current_User - %s', Current_User);
	Print FormatMessage (' XAct_State - %i', XAct_State());
	Print '--- Debug Data ---';

	-- Rollback Transaction
	Print 'Rollback Issued';
	Rollback Transaction;
	--Throw;
End Catch;</value>
